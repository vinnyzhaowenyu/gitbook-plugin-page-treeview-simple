{
  "name": "gitbook-plugin-page-treeview-simple",
  "version": "3.0.6",
  "description": "a gitbook plugin for generating a treeview of each page",
  "engines": {
    "gitbook": ">=3.0.0-pre.0"
  },
  "main": "./lib/index.js",
  "scripts": {
    "test": "./node_modules/.bin/mocha --reporter spec"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vinnyzhaowenyu/gitbook-plugin-page-treeview-simple.git"
  },
  "keywords": [
    "gitbook-plugin",
    "gitbook",
    "treeview"
  ],
  "author": "vinnyzhaowenyu <admin@zhaowenyu.com> (http://www.zhaowenyu.com)",
  "contributors": [
    "vinnyzhaowenyu <admin@zhaowenyu.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vinnyzhaowenyu/gitbook-plugin-page-treeview-simple/issues"
  },
  "homepage": "https://github.com/vinnyzhaowenyu/gitbook-plugin-page-treeview-simple#readme",
  "dependencies": {
    "remarkable": "1.6.2",
    "marked": "1.2.7",
    "node-html-parser": "1.2.20"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "mocha": "^3.0.2"
  },
  "gitbook": {
    "properties": {
      "copyright": {
        "type": "string",
        "default": "Copyright Â© aleen42",
        "title": "Description for your Copyright"
      },
      "minHeaderCount": {
        "type": "string",
        "default": "1",
        "description": "the minimum of headers"
      },
      "minHeaderDeep": {
        "type": "string",
        "default": "1",
        "description": "the minimum of the depth of headers"
      },
      "collapsed": {
        "type": "boolean",
        "default": false,
        "description": "whether collapse subheaders by default"
      }
    }
  }
}
